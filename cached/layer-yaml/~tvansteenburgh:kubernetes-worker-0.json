{
    "options": {
        "basic": {
            "packages": [
                "git",
                "build-essential",
                "software-properties-common",
                "nfs-common",
                "ceph-common",
                "socat"
            ],
            "use_venv": false,
            "include_system_packages": false
        },
        "tls-client": {
            "ca_certificate_path": "/srv/kubernetes/ca.crt",
            "server_certificate_path": "/srv/kubernetes/server.crt",
            "server_key_path": "/srv/kubernetes/server.key",
            "client_certificate_path": "/srv/kubernetes/client.crt",
            "client_key_path": "/srv/kubernetes/client.key"
        },
        "nagios": {},
        "nvidia-cuda": {},
        "debug": {},
        "docker": {
            "skip-install": false
        },
        "kubernetes-worker": {}
    },
    "includes": [
        "layer:basic",
        "interface:nrpe-external-master",
        "layer:debug",
        "layer:nagios",
        "interface:dockerhost",
        "interface:sdn-plugin",
        "interface:tls-certificates",
        "layer:docker",
        "layer:tls-client",
        "layer:nvidia-cuda",
        "interface:http",
        "interface:kubernetes-cni",
        "interface:kube-dns",
        "interface:kube-control"
    ],
    "repo": "https://github.com/kubernetes/kubernetes.git",
    "exclude": [
        "LAYER_README.md",
        "tests/10-deploy.py",
        "tests/tests.yaml"
    ],
    "is": "kubernetes-worker"
}